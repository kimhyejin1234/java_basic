package etc.api.lang.obj;

public class Person {
	private String name;
	private int age;
	
	public Person(String name,int age) {
		this.age = age;
		this.name = name;
	}

	
	@Override
	public String toString() {
		return "Person [name=" + name + ", age=" + age + "]";
	}


	public void personInfo() {
		System.out.println("이름 : " + name);
		System.out.println("나이 : " + age + "세");
	}

}
